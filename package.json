{
  "name": "@unonu/admin-layout",
  "version": "0.1.0",
  "private": false,
  "description": "a layout component of admin template, support vue2 and vue3",
  "author": "zgsgs (2087108700@qq.com)",
  "license": "MIT",
  "homepage": "https://github.com/zgsgs/uno-admin-layout#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zgsgs/uno-admin-layout.git"
  },
  "bugs": {
    "url": "https://github.com/zgsgs/uno-admin-layout/issues"
  },
  "keywords": [
    "admin",
    "layout",
    "admin-layout",
    "vue2",
    "vue3"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "npm run type:check && vite build",
    "build:vercel": "cross-env VITE_IS_VERCEL=1 vite build",
    "preview": "vite preview",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "publish": "npm publish --access=public",
    "release:publish": "npm release && pnpm publish",
    "type:check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json --max-warnings 0 --fix",
    "lint:fix": "lint-staged",
    "clear:dist": "rimraf dist"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.6",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@unocss/reset": "^0.45.6",
    "colord": "^2.9.3",
    "css-render": "0.15.10",
    "vue-demi": "^0.13.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@vitejs/plugin-vue": "^3.0.3",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.9.1",
    "eslint": "^8.22.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "standard-version": "^9.5.0",
    "typescript": "^4.7.4",
    "unocss": "^0.45.6",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-define-options": "^0.9.2",
    "vite": "3.0.7",
    "vite-plugin-dts": "^1.4.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.40.1"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
